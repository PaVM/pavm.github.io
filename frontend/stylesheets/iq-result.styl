
.iq-result {
  display none
}

.iq-result--visible {
  display block
}

.iq-result__title {
  display block
  margin-bottom 7px
  font-family ralewayMedium
  text-align center
  font-size 28px
  color #333
}

.iq-result__iq {
  display block
  font-family ralewayRegular
  text-align center
  font-size 20px
  color #333
}

.iq-result__iq--colored {
  font-size 24px
  color #3c8a3f
}
